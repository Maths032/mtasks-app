{
  "expo": {
    "name": "mtasks-app",
    "slug": "mtasks-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "statusBar": {
      "style": "light",
      "backgroundColor": "#110982"
    },
    "splash": {
      "statusBar": {
        "style": "light",
        "backgroundColor": "#110982"
      },
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#110982"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.maths32.mtasksapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "e9439f35-807c-4a10-9f71-31a3e9e60979"
      }
    }
  }
}
